  makeDistrusted' : (ps : List (Identifier √ó ParticipantModality))
                   ‚Üí PM (_ , UniqueByDec‚â° proj‚ÇÅ ps , isProp-UniqueBy _ _ )
                   ‚Üí ‚àÄ nm ‚Üí PM ( IsHonestParticipantId {ps} nm )  
                           ‚Üí  Œ£ (List (Identifier √ó ParticipantModality))
                                  Œª ps ‚Üí PM ( IsDistrustedParticipantId {ps} nm )
                                    √ó PM (_ , UniqueByDec‚â° proj‚ÇÅ ps , isProp-UniqueBy _ _ )
  makeDistrusted' l x nm w =
    ExistMemberAs-mapExisting-help {{eqTest (IsDiscrete-Identifier)}}
     l x w (Œª x‚ÇÅ q ‚Üí (proj‚ÇÅ x‚ÇÅ , distrusted) , proj‚ÇÅ q , refl)
       (Œª a _ ‚Üí refl)
       (Œª a a' x‚ÇÅ x‚ÇÇ ‚Üí sym (proj‚ÇÅ x‚ÇÅ) ‚àô proj‚ÇÅ x‚ÇÇ)
       Œª a a' x‚ÇÅ x‚ÇÇ x‚ÇÉ ‚Üí proj‚ÇÅ x‚ÇÅ ‚àô x‚ÇÇ ,
         sum-rec (Œª a‚ÇÅ ‚Üí empty-elim (x‚ÇÉ (proj‚ÇÅ x‚ÇÅ ‚àô x‚ÇÇ , (sym a‚ÇÅ)))) (sym) (dichotomyBool (proj‚ÇÇ a))  

  makeDistrustedŒ£ : (ih : InteractionHead) ‚Üí HonestParticipantId ih 
                           ‚Üí  Œ£ InteractionHead DistrustedParticipantId 
  AST.participantsWM (fst (makeDistrustedŒ£ ih hp)) =
     (fst (makeDistrusted' (AST.participantsWM ih) (AST.uniquePtcpnts ih) (pId-nameHon hp) (pId-isInHon hp)))
  AST.parameters (fst (makeDistrustedŒ£ ih hp)) = AST.parameters ih
  AST.uniqueParams (fst (makeDistrustedŒ£ ih hp)) = AST.uniqueParams ih
  AST.uniquePtcpnts (fst (makeDistrustedŒ£ ih hp)) = 
     proj‚ÇÇ (snd (makeDistrusted' (AST.participantsWM ih) (AST.uniquePtcpnts ih) (pId-nameHon hp) (pId-isInHon hp)))
  snd (makeDistrustedŒ£ ih hp) =
    AST.pId ((pId-nameHon hp))
       {proj‚ÇÅ (snd (makeDistrusted' (AST.participantsWM ih) (AST.uniquePtcpnts ih) (pId-nameHon hp) (pId-isInHon hp)))}

  makeDistrusted : (ih : InteractionHead) ‚Üí HonestParticipantId ih ‚Üí InteractionHead   
  makeDistrusted ih = fst ‚àò makeDistrustedŒ£ ih

